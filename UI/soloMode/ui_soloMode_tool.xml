            </Panel>
        </Panel>
    </Panel>

    <Image image="turnArrow" offsetXY="-50 2" preserveAspect="true" height="50" width="54" rectAlignment="UpperLeft" id="image_Arrow" knightPos="1" onClick="4d7f8e/b_moveTurnMarker"/>
<!-- Reset Turn Order ============================================================================================================================================ -->
    <Panel class="panel_brown" offsetXY="0 3" width="184" height="23" rectAlignment="LowerCenter" active="true" id="pannel_Open_SetTurnOrder">
        <Panel class="panel_black" width="178" height="17" rectAlignment="MiddleCenter">
            <Panel class="panel_grey" width="172" height="11" rectAlignment="MiddleCenter">
                <Button class="but_player" onClick="4d7f8e/b_TurnOrder_Open" id="but_Open_SetTurnOrder" />
                <Panel class="panel_grey" width="166" height="5" rectAlignment="MiddleCenter">
                </Panel>
            </Panel>
        </Panel>
    </Panel>

    <Panel class="panel_brown" offsetXY="0 3" width="184" height="30" rectAlignment="LowerCenter" active="false" id="pannel_Run_SetTurnOrder">
        <Panel class="panel_black" width="178" height="24" rectAlignment="MiddleCenter">
            <Panel class="panel_grey" width="172" height="18" rectAlignment="MiddleCenter">
                <Button class="but_player" onClick="4d7f8e/b_TurnOrder_Run" id="but_Run_SetTurnOrder" />
                <Panel class="panel_grey" width="166" height="12" rectAlignment="MiddleCenter">
                    <Text class="text_player" color="#FFFFFF" text="Set Turn Order"/>
                </Panel>
            </Panel>
        </Panel>
    </Panel>
<!-- END of Entrtre Pannel -->
</Panel>
