            <Panel class="panel_grey" width="172" height="28" rectAlignment="MiddleCenter"  id="pannel_player_3_out">
                <Button class="but_player" id="but_player_3" />
                <Panel class="panel_grey" width="166" height="22" rectAlignment="MiddleCenter" id="pannel_player_3_in">
                    <Text class="text_player" color="#FFFFFF" id="text_player_3"/>
                </Panel>
            </Panel>
        </Panel>
    </Panel>

    <Panel class="panel_brown" offsetXY="0 -114" width="184" height="40" rectAlignment="UpperCenter" active="false" knight="default" id="player_4">
        <Panel class="panel_black" width="178" height="34" rectAlignment="MiddleCenter">
            <Panel class="panel_grey" width="172" height="28" rectAlignment="MiddleCenter"  id="pannel_player_4_out">
                <Button class="but_player" id="but_player_4" />
                <Panel class="panel_grey" width="166" height="22" rectAlignment="MiddleCenter" id="pannel_player_4_in">
                    <Text class="text_player" color="#FFFFFF" id="text_player_4"/>
                </Panel>
            </Panel>
        </Panel>
    </Panel>

    <Panel class="panel_brown" offsetXY="0 -151" width="184" height="40" rectAlignment="UpperCenter" active="false" knight="default" id="player_5">
        <Panel class="panel_black" width="178" height="34" rectAlignment="MiddleCenter">
            <Panel class="panel_grey" width="172" height="28" rectAlignment="MiddleCenter"  id="pannel_player_5_out">
