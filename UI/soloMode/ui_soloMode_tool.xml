<Defaults>
    <Text class="Title" fontSize="30" alignment="Center" font="Fonts/MKCardTittle" color="#FFFFFF" /> <!-- fontStyle="Bold" -->
    <Text class="SettingTitle" alignment="Center" fontSize="22" font="Fonts/MKCardText" color="#000000" />

    <Panel class="panel_white" color="#FFFFFF" offsetXY="0 0" width="50" height="50" rectAlignment="MiddleCenter" />
    <Panel class="panel_grey" color="#757575" offsetXY="0 0" width="50" height="50" rectAlignment="MiddleCenter" />
    <Panel class="panel_brown" color="#926a41" offsetXY="0 0" width="50" height="50" rectAlignment="MiddleCenter" />
    <Panel class="panel_black" color="#000000" offsetXY="0 0" width="50" height="50" rectAlignment="MiddleCenter" />

    <Text class="text_player" color="Black" fontStyle="Bold" resizeTextForBestFit="true" text="Default"/>
    <Button class="but_player" colors="#757575|#dddddd|#444444|#00000000" resizeTextForBestFit="true" font="Fonts/MKCardText" textColor="#FFFFFF" onClick="4d7f8e/b_focusCamera"/>
</Defaults>

<Panel width="190" height="46" color="#000000" rectAlignment="UpperRight" active="false" raycastTarget="true" allowDragging="true" returnToOriginalPositionWhenReleased="false" id="panel_soloMode_Tool">

    <Panel class="panel_brown" offsetXY="0 -3" width="184" height="40" rectAlignment="UpperCenter" active="false" knight="default" id="player_1">
        <Panel class="panel_black" width="178" height="34" rectAlignment="MiddleCenter">
            <Panel class="panel_grey" width="172" height="28" rectAlignment="MiddleCenter"  id="pannel_player_1_out">
                <Button class="but_player" id="but_player_1" />
                <Panel class="panel_grey" width="166" height="22" rectAlignment="MiddleCenter" id="pannel_player_1_in">
                    <Text class="text_player" color="#FFFFFF" id="text_player_1"/>
                </Panel>
            </Panel>
        </Panel>
    </Panel>

    <Panel class="panel_brown" offsetXY="0 -40" width="184" height="40" rectAlignment="UpperCenter" active="false" knight="default" id="player_2">
