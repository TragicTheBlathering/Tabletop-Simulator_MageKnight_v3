                    <Text class="text_player" color="#FFFFFF" id="text_player_2"/>
                </Panel>
            </Panel>
        </Panel>
    </Panel>

    <Panel class="panel_brown" offsetXY="0 -77" width="184" height="40" rectAlignment="UpperCenter" active="false" knight="default" id="player_3">
